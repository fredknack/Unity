using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using TMPro;

public class ScoreManager : MonoBehaviour
{
    public TMP_Text textScore;
    public float score;

    void Start()
    {
        score = 0f;
        textScore.text = score.ToString() + " ITEMS";
    }
    void Update()
    {
        textScore.text = score.ToString() + " ITEMS";
    }
}
